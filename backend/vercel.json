{
    "version": 2,
    "functions": {
      "index.js": {
        "includeFiles": "*.*"
      }
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
    ]